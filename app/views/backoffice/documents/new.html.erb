<h1>Cadastrar Documentos e Identificação</h1>
<%= form_with(model: [ :backoffice, @user, @document ], local: true) do |form| %>

  <div class="field">
    <%= form.label :identity %>
    <%= form.text_field :identity %>
  </div>

  <div class="field">
    <%= form.label :expeditor %>
    <%= form.text_field :expeditor %>
  </div>

  <div class="field">
    <%= form.label :cpf %>
    <%= form.text_field :cpf %>
  </div>

  <div class="field">
    <%= form.label :nacionality %>
    <%= form.text_field :nacionality %>
  </div>

  <div class="field">
    <%= form.label :father_name %>
    <%= form.text_field :father_name %>
  </div>

  <div class="field">
    <%= form.label :mother_name %>
    <%= form.text_field :mother_name %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>